{
  "name": "js-dev-env",
  "version": "1.0.0",
  "description": "Dev env configuration to node",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruralkan/js-dev-env.git"
  },
  "author": "Manuel Perales",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ruralkan/js-dev-env/issues"
  },
  "homepage": "https://github.com/ruralkan/js-dev-env#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "cheerio": "^1.0.0-rc.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-watch": "^5.1.2",
    "express": "^4.17.1",
    "jsdom": "^15.1.1",
    "localtunnel": "^1.9.2",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "open": "^6.3.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
